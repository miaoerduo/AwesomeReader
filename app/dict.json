        "syns": [
            {
                "pl": "synonyms",
                "pt": [
                    [
                        "text",
                        "{sc}coarse{/sc} {sc}vulgar{/sc} {sc}gross{/sc} {sc}obscene{/sc} {sc}ribald{/sc} mean offensive to good taste or morals. {sc}coarse{/sc} implies roughness, rudeness, or crudeness of spirit, behavior, or language. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "found the {it}coarse{/it} humor of coworkers offensive"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}vulgar{/sc} often implies boorishness or ill-breeding. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "a loud {it}vulgar{/it} belch"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}gross{/sc} implies extreme coarseness and insensitiveness. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}gross{/it} eating habits"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}obscene{/sc} applies to anything strongly repulsive to the sense of decency and propriety especially in sexual matters. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}obscene{/it} language not allowed on the air"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}ribald{/sc} applies to what is amusingly or picturesquely vulgar or irreverent or mildly indecent. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "entertained the campers with {it}ribald{/it} folk songs"
                            }
                        ]
                    ]
                ]
            },
            {
                "pl": "synonyms",
                "pt": [
                    [
                        "text",
                        "{sc}flagrant{/sc} {sc}glaring{/sc} {sc}gross{/sc} {sc}rank{/sc} mean conspicuously bad or objectionable. {sc}flagrant{/sc} applies usually to offenses or errors so bad that they can neither escape notice nor be condoned. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}flagrant{/it} abuse of the office of president"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}glaring{/sc} implies painful or damaging obtrusiveness of something that is conspicuously wrong, faulty, or improper. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}glaring{/it} errors"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}gross{/sc} implies the exceeding of reasonable or excusable limits. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}gross{/it} carelessness"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}rank{/sc} applies to what is openly and extremely objectionable and utterly condemned. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}rank{/it} heresy"
                            }
                        ]
                    ]
                ]
            }
